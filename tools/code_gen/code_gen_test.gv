
[register_compound entity]

struct vec3 {
    float x;
    float y;
    float z;
};

struct vec2 {
    float x;
    float y;
};


entity player {
    [unpack]
    vec2 pos;
    int score;
};

entity enemy {
    [unpack]
    vec2 pos;
    float health;
};

entity missle {
    [unpack]
    vec2 pos;
    float dir_x;
    float dir_y;
};

entity obstacle {
    [unpack]
    vec2 pos;
    float size_x;
    float size_y;
};